<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auth.mapper.SysUsersRolesMapper">
    <resultMap id="BaseResultMap" type="com.auth.domain.SysUsersRoles">
        <result column="user_id" jdbcType="BIGINT" property="userId" />
        <result column="role_id" jdbcType="BIGINT" property="roleId" />
    </resultMap>
    <sql id="Base_Column_List">
        user_id, role_id
    </sql>


    <select id="selectByUserId" parameterType="java.lang.Long" resultType="com.auth.domain.SysUsersRoles">
        SELECT
        <include refid="Base_Column_List"/>
        FROM sys_users_roles
        WHERE user_id = #{userId}
    </select>

